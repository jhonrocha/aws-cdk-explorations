{
  "name": "aws-cdk-explorations",
  "version": "0.1.0",
  "bin": {
    "aws-cdk-explorations": "bin/aws-cdk-explorations.js"
  },
  "scripts": {
    "bootstrap": "cdk bootstrap",
    "start": "node ec2/fast-gql/index.js",
    "build": "esbuild ec2/fast-gql/index.js --minify --bundle --outdir=dist --platform=node --target=node14",
    "watch": "tsc -w",
