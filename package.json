    "synth": "cdk synth",
    "deploy": "cdk deploy",
    "deploy:cd": "cdk deploy --require-approval never",
    "db:migrate": "postgrator --config=./test/fixtures/postgrator/.postgratorrc.cjs"
  },
  "devDependencies": {
    "@types/jest": "^26.0.10",
    "@types/node": "10.17.27",
    "@typescript-eslint/eslint-plugin": "^5.13.0",
    "@typescript-eslint/parser": "^5.13.0",
    "aws-cdk": "2.13.0",
    "dotenv": "^16.0.0",
    "esbuild": "^0.14.23",
    "eslint": "^7.32.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.2.0",
    "jest": "^26.4.2",
    "postgrator-cli": "^5.1.0",
    "ts-jest": "^26.2.0",
    "ts-node": "^9.0.0",
    "typescript": "~3.9.7"
  },
  "dependencies": {
    "aws-cdk-lib": "2.13.0",
    "constructs": "^10.0.0",
    "fastify": "^3.27.2",
    "graphql": "^16.3.0",
